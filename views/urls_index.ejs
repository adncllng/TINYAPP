<!DOCTYPE html>
<html lang="en">
<head>
  <title>TinyApp</title>
</head>
<body>
  <header/>
  <%- include partials/_header %>
  </header>
  <ul>
  <% Object.keys(urls).forEach(url => { %>

    <li> <%= url %> : <%= urls[url] %>
      <form method="POST" action="/urls/<%=url%>/delete">
      <button type="submit" value="Delete">DELETE</button>
     </form>
     <a href='/urls/<%=url%>'>edit</a>
    </li>

  <% }) %>
  </ul>
  <p><a href='/urls/new'>Create a New Short Link</a></p>

</body>
</html>